<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Text?>

<GridPane fx:id="creditsGrid" fx:controller="de.hhu.propra.team61.gui.CreditsController" alignment ="CENTER" hgap="10" vgap="10" xmlns:fx="http://javafx.com/fxml">
    <children>
        <Text text="Developed by:" GridPane.columnIndex="0" GridPane.rowIndex="0"/>
        <Text text="Markus Brenneis, Simon Franz, Kevin Gnyp, Jan Ecknigk, Jessica Petrasch" GridPane.columnIndex="0" GridPane.rowIndex="1" GridPane.columnSpan="3"/>
        <Text text="2014" GridPane.columnIndex="0" GridPane.rowIndex="2"/>
        <Button text="Close" onAction="#handleClose" GridPane.columnIndex="2" GridPane.rowIndex="3" styleClass="mainButton"/>
    </children>
    <padding>
        <Insets bottom="25" left="25" right="25" top="25" />
    </padding>
</GridPane>